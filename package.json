{
  "name": "crypto-api",
  "version": "0.0.1",
  "description": "Basic api with interfaces to Bybit to get volume using Postgres",
  "main": "./index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start:dev": "DOTENV_CONFIG_PATH=.env ts-node-dev src/index.ts",
    "dev": "nodemon src/index.ts && DOTENV_CONFIG_PATH=.env ts-node-dev src/index.ts",
    "lint": "eslint --fix './src/**/*.ts'",
    "test": "DOTENV_CONFIG_PATH=.env.personal jest --detectOpenHandles --runInBand --forceExit --collect-coverage",
    "update-schema": "npx prisma migrate dev --preview-feature"
  },
  "engines": {
    "node": ">=12"
  },
  "engineStrict": true,
  "license": "UNLICENSED",
  "dependencies": {
    "@prisma/cli": "^2.17.0",
    "@prisma/client": "^2.17.0",
    "axios": "^0.19.2",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "https-proxy-agent": "^2.2.2",
    "lodash": "^4.17.19"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/helmet": "0.0.48",
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.159",
    "@types/node": "^14.11.5",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-typescript": "^10.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.2",
    "jest": "^26.4.2",
    "nodemon": "^2.0.7",
    "prisma": "^2.17.0",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.0",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^4.0.3"
  },
  "private": true,
  "gitHead": "da5d9d7a698f2076d8e389346aeaa68bc3afe70d"
